{
  "name": "flicklet-web",
  "private": true,
  "version": "0.1.167",
  "type": "module",
  "engines": {
    "node": ">=18 <21"
  },
  "scripts": {
    "dev": "vite",
    "prebuild": "npm run lexicon:build",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "check-all": "npm run typecheck && npm run lint",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:e2e": "cross-env VITEST=false playwright test",
    "test:e2e:ui": "cross-env VITEST=false playwright test --ui",
    "analyze:bundle": "npm run build && node scripts/bundle-budget-check.js",
    "lexicon:build": "node scripts/build-lexicon.mjs"
  },
  "dependencies": {
    "@sendgrid/mail": "^8.1.6",
    "@sentry/react": "^7.0.0",
    "@tanstack/react-query": "^5.56.2",
    "firebase": "^12.4.0",
    "firebase-admin": "^11.11.1",
    "lucide-react": "^0.546.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup-plugin-visualizer": "^6.0.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.48.2",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "jsdom": "^23.0.1",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.4.18",
    "typescript": "^5.0.0",
    "vite": "^4.4.0",
    "vitest": "^1.6.1"
  }
}
