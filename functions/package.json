{
  "name": "flicklet-functions",
  "version": "1.0.2",
  "description": "Firebase Cloud Functions for Flicklet",
  "main": "lib/src/index.js",
  "scripts": {
    "build": "tsc && node copy-assets.js",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "seed:titles": "ts-node --project tsconfig.json scripts/seedTitles.ts",
    "test:netlify": "node scripts/test-netlify-goofs-fetch.js",
    "check:insights": "node scripts/check-insights.js",
    "clear:insights": "node scripts/clear-insights.js"
  },
  "engines": {
    "node": "20"
  },
  "dependencies": {
    "@sendgrid/mail": "^8.1.6",
    "firebase-admin": "^11.11.1",
    "firebase-functions": "^6.6.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "firebase-functions-test": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.0"
  },
  "private": true
}
