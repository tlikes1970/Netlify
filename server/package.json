{
  "name": "flicklet-server",
  "version": "1.0.0",
  "description": "Flicklet community hub backend",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "dev": "node src/index.js",
    "migrate": "prisma migrate dev --name init_community",
    "generate": "prisma generate",
    "seed": "node prisma/seed.js",
    "pretest:server": "node scripts/test-setup.js && cross-env NODE_ENV=test pnpm prisma migrate deploy && node scripts/test-setup.js && cross-env NODE_ENV=test pnpm prisma db seed",
    "test:server": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --runInBand --detectOpenHandles --forceExit",
    "test:watch": "jest --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.19.0",
    "express": "^4.18.2",
    "firebase-admin": "^13.5.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/supertest": "^6.0.0",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "prisma": "^5.19.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  }
}
